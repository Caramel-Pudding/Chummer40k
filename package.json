{
  "name": "chummer40k",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev:vitals": "NEXT_PUBLIC_VITALS=true NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "eslint": "eslint ./ --ext .js,.jsx,.ts,.tsx --cache",
    "eslint:fix": "eslint ./ --ext .js,.jsx,.ts,.tsx --cache --fix",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "stylelint '**/*.css' --fix",
    "prisma:update-db": "prisma db push --preview-feature",
    "prisma:update-client": "prisma generate",
    "simple-git-hooks:update": "npx simple-git-hooks",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn eslint --quiet",
      "yarn stylelint"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn tsc --noEmit && yarn test && yarn audit && npx lint-staged"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@welldone-software/why-did-you-render": "^6.1.1",
    "autoprefixer": "^10.2.5",
    "axios": "^0.21.1",
    "classnames": "^2.3.0",
    "next": "10.1.3",
    "next-auth": "^3.11.3",
    "next-compose-plugins": "^2.2.1",
    "postcss": "^8.2.9",
    "postcss-flexbugs-fixes": "^5.0.1",
    "postcss-preset-env": "^6.7.0",
    "prisma": "^2.20.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-query": "^3.13.4",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "tailwindcss": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@next/bundle-analyzer": "^10.1.3",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.8",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.22",
    "@types/next-auth": "^3.13.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "simple-git-hooks": "^2.2.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.3",
    "webpack": "^4.0.0"
  }
}
